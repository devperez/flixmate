{
    "version": 2,
    "builds": [
        {"src": "package.json", "use": "@vercel/node"},
        {"src": "composer.json", "use": "@vercel/php"},
        {"src": "api/index.php", "use": "vercel-php@0.7.2"}
    ],
    "routes": [
        {"src": "/build/(.*)", "dest": "/public/build/$1"},
        {"src": "/js/(.*)", "dest": "/public/js/$1"},
        {"src": "/css/(.*)", "dest": "/public/css/$1"},
        {"src": "/images/(.*)", "dest": "/public/images/$1"},
        {"src": "/assets/(.*)", "dest": "/public/$1"},
        {"src": "/storage/(.*)", "dest": "/storage/$1"},
        {"src": "/(.*)", "dest": "/api/index.php"}
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_KEY": "base64:gh9fmRJEXH3TKfjS882AXDVyUJUmBQCwJrHW6tIh80I=",
        "APP_URL": "https://flixmates-vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}