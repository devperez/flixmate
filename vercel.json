{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/dist/js/$1"
        },
        {
            "src": "/assets/fonts/(.*)",
            "dest": "/dist/assets/fonts/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/dist/css/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/dist/images/$1"
        },
        {
            "src": "/manifest.json",
            "dest": "/dist/manifest.json"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_KEY": "base64:gh9fmRJEXH3TKfjS882AXDVyUJUmBQCwJrHW6tIh80I=",
        "APP_URL": "https://flixmate-vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "SSR_TEMP_PATH": "/tmp/ssr",
        "NODE_PATH" : "node"
    }
}
